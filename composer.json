{
    "name": "legless/php-followthemoney",
    "description": "PHP library to work with Follow the Money entities",
    "type": "library",
    "require": {
        "php": ">=7.4",
        "ext-yaml": "*",
        "ext-json": "*"
    },
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "FollowTheMoney\\": "src"
        }
    },
    "autoload-dev": {
        "psr-1": {
            "FollowTheMoney\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --testdox --colors"
    }
}
